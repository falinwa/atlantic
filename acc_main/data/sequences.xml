<odoo>
    <record id="sequence_confirmed" model="ir.sequence">
        <field name="name">Confirmed Sale Orders</field>
        <field name="code">confirmed.sale</field>
        <field name="active">TRUE</field>
        <field name="prefix"/>
        <field name="padding">5</field>
        <field name="number_increment">1</field>
    </record>
    <record id="seq_sale_order" model="ir.sequence">
        <field name="name">Sales Order</field>
        <field name="code">sale.order</field>
        <field name="active">TRUE</field>
        <field name="prefix">DEV.%(y)s.</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
    <record id="seq_purchase_order" model="ir.sequence">
        <field name="name">Purchase Order</field>
        <field name="code">purchase.order</field>
        <field name="active">TRUE</field>
        <field name="prefix">CF.%(y)s.</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>
